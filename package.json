{
    "name": "playground",
    "version": "99.99.99",
    "license": "WTFPL",
    "browserslist": [
        "last 2 major versions and >1%",
        "not ie<=11",
        "not op_mini all"
    ],
    "scripts": {
        "clean": "rm -rf dist .parcel-cache",
        "build": "parcel build index.html --public-url='/'",
